{
  "name": "project-roadmap-tracking",
  "description": "CLI based project task tracking",
  "version": "0.2.2",
  "author": "ZacharyEggert",
  "bin": {
    "prt": "./bin/run.js"
  },
  "bugs": "https://github.com/ZacharyEggert/project-roadmap-tracking/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "ajv": "^8.12.0",
    "chokidar": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "c8": "^10.1.3",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3.8.1",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/ZacharyEggert/project-roadmap-tracking",
  "keywords": [
    "oclif",
		"cli",
		"project",
		"roadmap",
		"tracking",
		"tasks",
		"productivity",
		"management",
		"prt"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "prt",
    "dirname": "prt",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "ZacharyEggert/project-roadmap-tracking",
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --write",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "c8 mocha --loader=tsx/esm --forbid-only 'test/**/*.test.ts'",
    "test:coverage": "c8 report",
    "test:coverage:summary": "c8 report --reporter=text",
    "version": "oclif readme && git add README.md"
  },
  "c8": {
    "reporter": [
      "lcov",
      "html"
    ],
    "reports-dir": "./coverage"
  },
  "types": "dist/index.d.ts"
}
